<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:context=".ui.foundation.contentproviders.ContentProvidersActivity">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/call_10086"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="拨打10086"
            android:theme="@style/buttonStyle"
            app:cornerRadius="12dp"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/myContentProviderBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="自定义ContentProvider"
            android:theme="@style/buttonStyle"
            app:cornerRadius="12dp"
            app:layout_constraintTop_toBottomOf="@+id/call_10086" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
